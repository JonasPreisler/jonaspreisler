<p id="notice"><%= notice %></p>

<h1>Misschilis</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @misschilis.each do |misschili| %>
      <tr>
        <td><%= misschili.product_id %></td>
        <td><%= link_to 'Show', misschili %></td>
        <td><%= link_to 'Edit', edit_misschili_path(misschili) %></td>
        <td><%= link_to 'Destroy', misschili, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Misschili', new_misschili_path %>
